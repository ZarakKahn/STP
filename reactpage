const fetchData = async () => {
  try {
    const result = await axios.post(
      `http://portal.mashitec.com/SalesWebApi/api/Gethierarchy/?num=${encodeURIComponent(num)}&role=SaleMan`,
    );
    if (result.status === 200) {
      if (result.data.length > 0) {
        setMessage("Number Correct");
        setData(result.data);
      } else {
        setMessage("No data found for the entered number.");
        setData([]);
      }
    } else {
      setMessage("Error: Something went wrong.");
      setData([]);
    }
  } catch (error) {
    setMessage("Error: " + error.msg);
    setData([]);
  }
};
